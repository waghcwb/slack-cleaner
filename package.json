{
  "name": "slack-cleaner",
  "version": "1.0.0",
  "description": "Delete all messages from user and channel",
  "main": "dist/app.bundle.js",
  "scripts": {
    "dev": "./node_modules/.bin/nodemon node dist",
    "build": "./node_modules/.bin/tsc --project tsconfig.json",
    "build:dev": "./node_modules/.bin/tsc --project tsconfig.json --watch",
    "start": "node dist init",
    "clean": "./node_modules/.bin/rimraf dist"
  },
  "keywords": [
    "slack",
    "delete",
    "clear",
    "remove",
    "messages"
  ],
  "author": "waghcwb <wagh.cwb@gmail.com>",
  "license": "MIT",
  "files": ["dist"],
  "types": "src/index.d.ts",
  "dependencies": {
    "axios": "^0.19.0",
    "consola": "^2.7.1",
    "typescript": "^3.5.1"
  },
  "devDependencies": {
    "@types/node": "^12.0.4",
    "@types/webpack": "^4.4.32",
    "commander": "^2.20.0",
    "dotenv": "^8.0.0",
    "nodemon": "^1.19.1",
    "prettier": "^1.17.1",
    "rimraf": "^2.6.3"
  }
}
